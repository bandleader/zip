{
  "name": "zip",
  "version": "0.2.7",
  "description": "Framework for quick web apps",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist",
    "default-files/*",
    "cmds/*"
  ],
  "types": "dist/zip-runner.d.ts",
  "bin": { 
    "zip-build": "cmds/zip-build.js", 
    "zip-serve": "cmds/zip-serve.js", 
    "zip-init": "cmds/zip-init.js" 
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test-serve": "ts-node-dev --respawn --transpile-only --compiler-options \"{\\\"module\\\": \\\"CommonJS\\\"}\" ./server.ts"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@types/socket.io": "^2.1.4",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.6",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "rollup": "^2.23.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "express": "^4.16.3",
    "typescript": "^3.8.3"
  }
}
